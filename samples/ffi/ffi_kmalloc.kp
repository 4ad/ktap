#!/usr/bin/env ktap

ffi.cdef[[
	typedef unsigned gfp_t;
	typedef unsigned long size_t;
	void *__kmalloc( size_t size, gfp_t flags);
	void kfree(const void *objp);
]]

local t1 = gettimeofday_us()

for (i = 1, 1000, 1) {
	local object = ffi.C.__kmalloc(128, 208) #GFP_KERNEL is 208
	ffi.C.kfree(object)
}

local t2 = gettimeofday_us()

printf("execution time: %d us\n", t2 - t1)
