#!/usr/bin/env ktap

#this script output each average consumimg time of each softirq line
var s = ptable()
var map = {}

trace irq:softirq_entry {
	map[cpu()] = gettimeofday_us()
}

trace irq:softirq_exit {
	var entry_time = map[cpu()]
	if (entry_time == nil) {
		return;
	}

	s[arg1] <<< gettimeofday_us() - entry_time
	map[cpu()] = nil
}

trace_end {
	print(s)
}

