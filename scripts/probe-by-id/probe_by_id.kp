# See kprobe.sh/uprobe.sh to know how to use this ktap script
# this script can serve as tracepoint/kprobe/kretprobe/uprobe/uretprobe
function eventfun (e) {
	printf("%d %d\t%s\t%s", cpu(), pid(), execname(), e.tostring())
}


kdebug.probe_by_id(arg[1], eventfun)

kdebug.probe_end(function () {
	printf("probe end\n")
})

